#include <stdio.h>
int main() {
    int n, flag = 1;
    printf("Enter a number: ");
    scanf("%d", &n);
    if(n <= 1) flag = 0;
    for(int i = 2; i*i <= n; i++) {
        if(n % i == 0) {
            flag = 0;
            break;
        }
    }
    printf("%d is %s\n", n, flag ? "Prime" : "Not Prime");
    return 0;
}
