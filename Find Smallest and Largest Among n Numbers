#include <stdio.h>
int main() {
    int n, num, largest, smallest;
    printf("Enter how many numbers: ");
    scanf("%d",&n);
    scanf("%d",&num);
    largest = smallest = num;
    for(int i=2;i<=n;i++){
        scanf("%d",&num);
        if(num>largest) largest=num;
        if(num<smallest) smallest=num;
    }
    printf("Largest=%d, Smallest=%d\n", largest, smallest);
    return 0;
}
