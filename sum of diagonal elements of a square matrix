#include <stdio.h>

int main() {
    int a[10][10], n, sum = 0;

    printf("Enter the order of square matrix: ");
    scanf("%d", &n);

    printf("Enter elements:\n");
    for(int i=0;i<n;i++)
        for(int j=0;j<n;j++)
            scanf("%d",&a[i][j]);

    for(int i=0;i<n;i++)
        sum += a[i][i];  // main diagonal elements

    printf("Sum of diagonal elements = %d\n", sum);
    return 0;
}
